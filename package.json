{
  "name": "vue-paintable",
  "version": "0.4.6",
  "description": "Draw on any screen",
  "keywords": [
    "canvas",
    "vue",
    "paintable",
    "touch",
    "mouse"
  ],
  "main": "dist/index.js",
  "author": "Ph1p <phil@capshake.com>",
  "repository": "https://github.com/ph1p/vue-paintable.git",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "MIT",
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server -d --open --progress --public --host 0.0.0.0",
    "start": "NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.js",
    "prepublish": "npm start"
  },
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.2",
    "node-sass": "^4.9.0",
    "postcss-color-function": "^3.0.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "sass-loader": "^7.0.1",
    "scss-loader": "0.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "vue": "^2.5.16",
    "vue-cli": "^2.8.1",
    "vue-loader": "^11.3.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "3.10.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^2.11.2"
  }
}
